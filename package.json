{
  "name": "mongres",
  "version": "0.0.1",
  "description": "ORM for Postgres",
  "engines": {
    "node": ">=8"
  },
  "main": "src/index.js",
  "scripts": {
    "format": "prettier --write \"*/**/*.{js,md}\"",
    "lint": "eslint -c .eslintrc src/**/*.js && eslint -c test/.eslintrc test/**/*.js ",
    "police": "police -l",
    "posttest": "npm run lint",
    "precommit": "lint-staged",
    "setup": "bash test/setup.bash",
    "test": "NODE_ENV=test mocha --opts ./test/test.opts test/tests"
  },
  "keywords": [
    "orm",
    "relational",
    "sql",
    "postgres"
  ],
  "author": "Dave Kelly <dmk3534@gmail.com>",
  "license": "MIT",
  "repository": "dmkelly/mongres",
  "homepage": "https://github.com/dmkelly/mongres",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "6.1.0",
    "husky": "3.0.2",
    "lint-staged": "9.2.1",
    "mocha": "6.2.0",
    "pg": "7.12.0",
    "police": "0.6.0",
    "prettier": "1.18.2",
    "sinon": "7.4.1"
  },
  "dependencies": {
    "knex": "0.19.1",
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "pg": ">=7"
  }
}
